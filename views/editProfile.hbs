<div class="conteiner center">{{!-- Inicia Conteiner Principal --}}
  <div class="row center">
    <form class="col s10 offset-s1">
      <div class="row">
        <div class="input-field col s12">
          <input placeholder="Placeholder" id="username" type="text">
          <label for="username">Name</label>
        </div>
      </div>

      <div class="row">
        <div class="input-field col s12">
          <input placeholder="Write About yourself" id="user_aboutme" type="text">
          <label for="user_aboutme">About me</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input placeholder="Telephone" id="phone" type="text">
          <label for="phone">Phone Number</label>
        </div>
      </div>

      <div class="row">
        <div class="input-field col s12">
          <input id="password" type="password" class="password">
          <label for="password">Password</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="email" type="email" class="validate">
          <label for="email">Email</label>
        </div>
      </div>

      <div class="row">

        <div class="input-field col s12">
          <select name="Your artist essence">
            <option value="" disabled selected>Choose your art </option>
            <option value="Actor">Actor</option>
            <option value="Painter">Painter</option>
            <option value="Model">Model</option>
            <option value="Singer">Singer</option>
            <option value="Musician">Musician</option>
            <option value="Videographer">Videographer</option>
            <option value="Painter">Painter</option>
            <option value="Designer">Designer</option>
            <option value="Cinematographer">Cinematographer</option>
            <option value="Writter">Writter</option>
            <option value="Digital Artist">Digital Artist</option>
          </select>
          <label>Materialize Select</label>

          <div>
            <div class="chips col s12 left"></div>
          </div>
          <button class="btn waves-effect waves-light" type="submit" name="action">Submit
            <i class="material-icons right">send</i>
          </button>

        </div>
      </div>
    </form>
  </div>
</div> {{!-- Finaliza conteiner Principal  --}}